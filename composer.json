{
    "name": "raojeet/textlocal",
    "description": "Laravel Text Local Package",
    "license": "MIT",
    "keywords": [
        "laravel textlocal",
        "textlocal gateway",
        "textlocal sms gateway",
        "text local",
        "laravel textlocal package"
    ],
    "authors": [
        {
            "name": "raojeet",
            "email": "jeet.yadav25@gmail.com",
            "homepage": "https://github.com/raojeet",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/raojeet/textlocal",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\TextLocal\\" : "src"
        }
    },
    "extra": {
         "laravel": {
            "providers": [
                "App\\TextLocal\\TextLocalServiceProvider"
            ],
            "aliases": {
                "TextLocal": "App\\TextLocal\\Facades\\TextLocal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
